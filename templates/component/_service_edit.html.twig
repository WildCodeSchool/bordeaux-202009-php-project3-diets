<div>
    <h3 class="part-title">Mes fiches :</h3>
</div>
<div class="d-flex flex-wrap justify-content-center">
    {% for resource in resources %}
        <div class="card-container justify-content-center">
            <div>
                {% include '/component/_card.html.twig' %}
            </div>
        </div>
    {% endfor %}
</div>

<div>
    <h3 class="part-title">Mes événements :</h3>
</div>
<div class="d-flex flex-wrap justify-content-center">
    {% for i in 20..23 %}
        <div class="card-container justify-content-center">
            <div class="overflow-hidden">
                <img src="https://picsum.photos/400/200?random={{ i }}" alt='événement organisé'>
            </div>
        </div>
    {% endfor %}
</div>

<div>
    <h3 class="part-title">Mes services :</h3>
</div>
<div class="d-flex flex-wrap justify-content-center">
    {% for j in 30..33 %}
        <div class="card-container justify-content-center">
            <div class="overflow-hidden">
                <img src="https://picsum.photos/400/200?random={{ j }}" alt='événement organisé'>
            </div>
        </div>
    {% endfor %}
</div>
</div>
